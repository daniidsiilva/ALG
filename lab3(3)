#include <stdio.h>
int main() {
  int number;

  printf("вкажіть номер квартири: ");
  scanf("%d", &number);

  if (number <= 0 || number > 45) {
    printf("не існує\n");
    return 1;
  }

  switch (number) {
    case 1:
    case 2:
    case 3:
    case 4:
    case 5:
      printf("перший поверх\n");
      break;

    case 6:
    case 7:
    case 8:
    case 9:
    case 10:
      printf("другий поверх\n");
      break;

    case 11:
    case 12:
    case 13:
    case 14:
    case 15:
      printf("третій поверх\n");
      break;

    case 16:
    case 17:
    case 18:
    case 19:
    case 20:
      printf("четвертий поверх\n");
      break;

    case 21:
    case 22:
    case 23:
    case 24:
    case 25:
      printf("п'ятий поверх\n");
      break;

    case 26:
    case 27:
    case 28:
    case 29:
    case 30:
      printf("шостий поверх\n");
      break;

    case 31:
    case 32:
    case 33:
    case 34:
    case 35:
      printf("сьомий поверх\n");
      break;

    case 36:
    case 37:
    case 38:
    case 39:
    case 40:
      printf("восьмий поверх\n");
      break;

    case 41:
    case 42:
    case 43:
    case 44:
    case 45:
      printf("дев'ятий поверх\n");
      break;

    default:
      printf("не існує\n");
      break;
    }
  return 0;
}
